;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

[Version]
Signature	= "$Windows NT$"
Class		= HeKA_DAQ_Driver				; Heka_DAQ
ClassGUID 	= {3E451B31-041C-11d5-8453-00E02954BA20}
Provider	= %ProviderName%				; HEKA
DriverVer	= 01/19/2012,22.5
CatalogFile.NTx86 = HEKADAQx86.cat
CatalogFile.NTAMD64 = HEKADAQamd64.cat
CatalogFile.NTIA64 = HEKAia64.cat

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[ClassInstall32.NTx86]
Addreg = ITCClassReg

[ClassInstall32.NTamd64]
Addreg = ITCClassReg

[ITCClassReg]
HKR,,,0,%ClassName%						; Heka_DAQ
HKR,,Icon,,-5
;HKR,,Security,,"D:P(A;;GA;;;SY)(A;;GRGWGX;;;BA)(A;;GR;;;WD)(A;;GR;;;RC)"
HKR,,Security,,"D:P(A;;GA;;;SY)(A;;GRGWGX;;;BA)(A;;GRGWGX;;;WD)(A;;GRGWGX;;;RC)"

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
