%ManufactureName%   = ITC,NTx86,NTamd64			; HEKA Instruments Inc.

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
[ITC.NTx86]
%ITC16%		= ITC16, PCI\VEN_1482&DEV_0001&SUBSYS_00000000
%ITC18%		= ITC18, PCI\VEN_1482&DEV_0002&SUBSYS_00000000
%ITC181%	= ITC18, PCI\VEN_1482&DEV_0003&SUBSYS_00000000
%ITC1600%	= ITC1600, PCI\VEN_1057&DEV_1801&SUBSYS_00401482
%USB16%		= USB16, USB\VID_1482&PID_0010
%USB18%		= USB18, USB\VID_1482&PID_0011

[ITC.NTamd64]
%ITC16%		= ITC16, PCI\VEN_1482&DEV_0001&SUBSYS_00000000
%ITC18%		= ITC18, PCI\VEN_1482&DEV_0002&SUBSYS_00000000
